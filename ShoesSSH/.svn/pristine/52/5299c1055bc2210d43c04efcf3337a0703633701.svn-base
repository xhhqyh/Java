package com.shoe.pojo;

/**
 * 积分表(t_point)
 * 
 * @author 李杨
 * @param pointId
 *            Integer ID,主键标识符
 * @param pointCurrent
 *            Double 现有积分
 * @param pointTotal
 *            Double 累计积分
 * @param userId
 *            Integer 用户编号,外键,链接用户表(t_user)
 */
public class Point {
	private Integer pointId;
	private Double pointCurrent;
	private Double pointTotal;
	private Integer userId;

	public Integer getPointId() {
		return pointId;
	}

	public void setPointId(Integer pointId) {
		this.pointId = pointId;
	}

	public Double getPointCurrent() {
		return pointCurrent;
	}

	public void setPointCurrent(Double pointCurrent) {
		this.pointCurrent = pointCurrent;
	}

	public Double getPointTotal() {
		return pointTotal;
	}

	public void setPointTotal(Double pointTotal) {
		this.pointTotal = pointTotal;
	}

	public Integer getUserId() {
		return userId;
	}

	public void setUserId(Integer userId) {
		this.userId = userId;
	}

	@Override
	public String toString() {
		return "Point [pointId=" + pointId + ", pointCurrent=" + pointCurrent + ", pointTotal=" + pointTotal
				+ ", userId=" + userId + "]";
	}

}
