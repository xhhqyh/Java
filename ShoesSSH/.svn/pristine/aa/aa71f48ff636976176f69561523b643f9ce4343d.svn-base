<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.shoe.pojo">
	<class name="Cooperation" table="t_cooperation">
		<id name="coId" column="co_id">
			<generator class="sequence">
				<param name="sequence_name">cooperation_seq</param>
			</generator>
		</id>
		
		<property name="coName" column="co_name"></property>
		<property name="coUrl" column="co_url"></property>
		<property name="cpImgUrl" column="co_imgurl"></property>
		<property name="coStatus" column="co_status"></property>
	</class>
</hibernate-mapping>