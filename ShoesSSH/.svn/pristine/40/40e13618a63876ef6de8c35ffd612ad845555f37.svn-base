package com.shoe.pojo;

import com.shoe.enums.Sex;

/**
 * 用户表(t_user)
 * 
 * @author Administrator
 * @param userId
 *            Integer 用户ID,主键标识符
 * @param userAccount
 *            String 用户账号
 * @param userPwd
 *            String 用户密码
 * @param userName
 *            String 用户真实姓名
 * @param userTel
 *            String 用户联系方式
 * @param userSex
 *            Integer 性别
 * @param userEmail
 *            String 邮箱
 * @param userDate
 *            String 用户注册时间
 * @param securityId
 *            Integer 密保问题 外键链接密保信息表(t_security)
 * @param userAnswer
 *            String 用户答案
 * @param userStatus
 *            Integer 用户状态,默认正常 0:正常,1:封禁状态
 */
public class User {
	private Integer userId;
	private String userAccount;
	private String usePwd;
	private String userName;
	private String userTel;
	private Integer userSex;
	private String userEmail;
	private String userDate;
	private Integer securityId;
	private String userAnswer;
	private Integer userStatus;

	public Integer getUserId() {
		return userId;
	}

	public void setUserId(Integer userId) {
		this.userId = userId;
	}

	public String getUserAccount() {
		return userAccount;
	}

	public void setUserAccount(String userAccount) {
		this.userAccount = userAccount;
	}

	public String getUsePwd() {
		return usePwd;
	}

	public void setUsePwd(String usePwd) {
		this.usePwd = usePwd;
	}

	public String getUserName() {
		return userName;
	}

	public void setUserName(String userName) {
		this.userName = userName;
	}

	public String getUserTel() {
		return userTel;
	}

	public void setUserTel(String userTel) {
		this.userTel = userTel;
	}

	public Integer getUserSex() {
		return userSex;
	}

	public void setUserSex(Integer userSex) {
		this.userSex = userSex;
	}

	public String getUserEmail() {
		return userEmail;
	}

	public void setUserEmail(String userEmail) {
		this.userEmail = userEmail;
	}

	public String getUserDate() {
		return userDate;
	}

	public void setUserDate(String userDate) {
		this.userDate = userDate;
	}

	public Integer getSecurityId() {
		return securityId;
	}

	public void setSecurityId(Integer securityId) {
		this.securityId = securityId;
	}

	public String getUserAnswer() {
		return userAnswer;
	}

	public void setUserAnswer(String userAnswer) {
		this.userAnswer = userAnswer;
	}

	public Integer getUserStatus() {
		return userStatus;
	}

	public void setUserStatus(Integer userStatus) {
		this.userStatus = userStatus;
	}

	@Override
	public String toString() {
		return "User [userId=" + userId + ", userAccount=" + userAccount + ", usePwd=" + usePwd + ", userName="
				+ userName + ", userTel=" + userTel + ", userSex=" + userSex + ", userEmail=" + userEmail
				+ ", userDate=" + userDate + ", securityId=" + securityId + ", userAnswer=" + userAnswer
				+ ", userStatus=" + userStatus + "]";
	}

}
