<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	 <class name="com.shoe.pojo.Push" table="t_push">
		<id name="pushId" column="push_id">
			<generator class="sequence">
				<param name="sequence_name">push_seq</param>
			</generator>
		</id>
		<property name="pushName" column="push_name"></property>
		<property name="pushImg" column="push_img"></property>
		<property name="pushStatus" column="push_status"></property>
		
	</class>
	

</hibernate-mapping>

